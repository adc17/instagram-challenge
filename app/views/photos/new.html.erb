<h1>Upload to the PhotoFeed</h1>
<%= form_for :photo, url: photos_path, html: {multipart: true} do |f| %>
  <p>
    <%= f.label :title, "Image Title:" %><br>
    <%= f.text_field :title %><br>
  </p>

  <p>
    <%= f.label :image, "Upload your image:" %>
    <%= f.file_field :image %>
  </p>

  <p>
    <%= f.submit "Upload" %>
  </p>
<% end %>

